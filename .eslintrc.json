{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-shadow": [0],
    "import/no-unresolved": [0],
    "import/no-duplicates": [0]
  }
}
